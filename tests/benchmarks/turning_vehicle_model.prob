v0 = 10
t = 0.1
q = -0.5
psi = Normal(0, 0.01)
cos_psi = Cos(psi)
sin_psi = Sin(psi)
v = Uniform(6.5, 8.0)
x = Uniform(-.1, .1)
y = Uniform(-.5, -.3)
while true:

    w1 = Uniform(-0.1, 0.1)
    w2 = Normal(0, 0.01)
    cos_w2 = Cos(w2)
    sin_w2 = Sin(w2)

    x = x + t * v * cos_psi
    y = y + t * v * sin_psi

    v = v + t*(q*(v - v0) + w1)
    psi = psi + w2
    cos_psi, sin_psi = cos_psi*cos_w2 - sin_psi*sin_w2, sin_psi*cos_w2 + cos_psi*sin_w2
end

