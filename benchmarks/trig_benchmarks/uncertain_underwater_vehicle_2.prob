x = Uniform(-0.1, 0.1)
y = Uniform(-0.1, 0.1)
theta = Uniform(pi/4 - 0.1, pi/4 + 0.1)
cos_theta = Cos(theta)
sin_theta = Sin(theta)
while true:
    omega = Beta(1, 3)

    g = Normal(0, 0.01)
    sin_g = Sin(g)
    cos_g = Cos(g)

    theta = theta + g
    cos_theta, sin_theta = cos_theta*cos_g - sin_theta*sin_g, sin_theta*cos_g + cos_theta*sin_g

    x = x + 0.1*(2 + omega)*cos_theta
    y = y + 0.1*(2 + omega)*sin_theta
end
