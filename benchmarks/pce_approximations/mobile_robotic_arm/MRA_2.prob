x = 0
y = 0
z = 0

while true:
    t1 = Uniform(-0.1, 0.1)
    t2 = Normal(0.7853981, 1)
    t3 = Gamma(1, 2)
    q_x = Uniform(-0.1, 0.1)
    q_y = Normal(0, 1)
    q_z = Beta(3, 1)
    c_t1 = -0.499642956334181*t1**2 + 0.999999642989395
    c_t2 = -0.214440971240177*t2**2 - 0.0920388525419539*t2 + 0.847888031563559
    s_t1 = 0.999000357076727*t1
    s_t2 = -0.214440971240177*t2**2 + 0.765725032418753*t2 + 0.17420185168676
    s_t3 = 0.00800000000033245*t3**2 - 0.184000000002559*t3 + 0.704000000002462
    c_t3 = 0.0440000000008693*t3**2 - 0.512000000006759*t3 + 0.87200000000656
    s_t23 = s_t2 * c_t3 + c_t2 * s_t3
    c_t23 = c_t2 * c_t3 - s_t2 * s_t3
    x = q_x + c_t1*(0.5 * c_t23 + c_t2)
    y = q_y + s_t1 * (0.5 * c_t23 + c_t2)
    z = q_z + 0.5 * s_t23 + s_t2
end
