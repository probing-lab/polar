a_p = 0.5
a_y = 0.5
y = 1
y1 = 1
p = 0.01
p1 = 0.01
i = 0.02
r = 0.015
while true:
    dp = Normal(0, 0.01)
    dy = DistExp(100)
    p = p1
    p1 = p + dp
    y1 = 0.01 + 1.02*y
    y  = y1 - dy
    l_y  = 3.93245289610017e-9*y**9 - 3.73050510986517e-7*y**8 + 1.4377528849917e-5*y**7 - 0.000294135789876289*y**6 + 0.00351428216581411*y**5 - 0.0256019836446732*y**4 + 0.117284665473828*y**3 - 0.361160106665549*y**2 + 0.95730570696661*y + 0.00331703523374669
    l_y1 = 3.93245289610017e-9*y1**9 - 3.73050510986517e-7*y1**8 + 1.4377528849917e-5*y1**7 - 0.000294135789876289*y1**6 + 0.00351428216581411*y1**5 - 0.0256019836446732*y1**4 + 0.117284665473828*y1**3 - 0.361160106665549*y1**2 + 0.95730570696661*y1 + 0.00331703523374669
    i = r + p + a_p*(p - p1) + a_y*(l_y - l_y1)
end