a_p = 0.5
a_y = 0.5
y = 1
y1 = 1
p = 0.01
p1 = 0.01
i = 0.02
r = 0.015
while true:
    dp = Normal(0, 0.01)
    dy = Exponential(100)
    p = p1
    p1 = p + dp
    y1 = 0.01 + 1.02*y
    y  = y1 - dy
    l_y  = 0.00687456360901706*y**3 - 0.114610550721409*y**2 + 0.738352695654612*y + 0.0459683868684806
    l_y1 = 0.00687456360901706*y1**3 - 0.114610550721409*y1**2 + 0.738352695654612*y1 + 0.0459683868684806
    i = r + p + a_p*(p - p1) + a_y*(l_y - l_y1)
end