m0 = 10000
lam0 = 0.10

m = m0
lam = 0
s = 0

while true:
    s = Normal(lam0, 0.0625)
    lam = lam + s
    t = -lam
    e_l = 0.00228989065374931*t**6 + 0.013739343922501*t**5 + 0.0343483598062657*t**4 + 0.137393439225012*t**3 + 0.515225397093751*t**2 + 1.03045079418758*t + 0.996102434381341
    m = m0 * e_l
end