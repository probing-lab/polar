psi = Beta(3, 3)
theta = Beta(1, 3)
x = Uniform(-0.1, 0.1)
y = Uniform(-0.1, 0.1)
z = Uniform(0.1, 0.3)

while true:
    o_v = Beta(1, 3)
    o_p = Uniform(-0.1, 0.1)
    o_t = Normal(0, 0.3)
    

    c_p = 7.75047079612101e-7*psi**8 - 4.9603009473186e-5*psi**7 + 0.00112846810161293*psi**6 - 0.0104166164776583*psi**5 + 0.0208330159419036*psi**4 + 0.208334358095849*psi**3 - 0.906251546974135*psi**2 + 0.250000887589974*psi + 0.968749888830095
    s_p = 7.75046897390026e-7*psi**8 - 6.2003794903808e-5*psi**7 + 0.00190971798053896*psi**6 - 0.0286457826049019*psi**5 + 0.21614551795611*psi**4 - 0.739582331202524*psi**3 + 0.687498509683082*psi**2 + 0.687500843123597*psi + 0.0312498957913568
    c_t = 1.50429231081831e-5*theta**8 - 0.00126360554107428*theta**6 + 0.0410671800847554*theta**4 - 0.499124188721943*theta**2 + 0.999827884370066
    s_t = -0.000120343384863778*theta**7 + 0.00758163324641143*theta**5 - 0.164268720338856*theta**3 + 0.998248377443728*theta
    x = x + 0.1*(1 + o_v)*c_p * c_t
    y = y + 0.1*(1 + o_v)*s_p * c_t
    z = z + 0.1*(1 + o_v)*s_t
    theta = theta + 0.1 * (1 + o_t)
    psi = psi + 0.1 * (1 + o_p)
end