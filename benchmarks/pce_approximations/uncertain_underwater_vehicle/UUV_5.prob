x = Uniform(-0.1, 0.1)
y = Uniform(-0.1, 0.1)
t = Uniform(pi/4 - 0.1, pi/4 + 0.1)

while true:
    o_v = Beta(1, 3)
    shock = Normal(0, 1)
    t = t + 0.1 * shock
    c_t = 0.0252721108213598*t**4 - 0.454897994784475*t**2 + 0.985612322033029
    s_t = 0.00505442216427183*t**5 - 0.151632664928157*t**3 + 0.985612322033028*t
    x = x + 0.1 * (2 + o_v)*c_t
    y = y + 0.1 * (2 + o_v)*s_t

end
